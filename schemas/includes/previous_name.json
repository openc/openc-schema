{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "name": "PreviousName",
  "description": "A previous name of a company",
  "type": "object",
  "properties": { "company_name": { "type": "string", "minLength": 1 },
                  "con_date": {
                    "type": "string",
                    "format": "date",
