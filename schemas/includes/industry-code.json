{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "An industry code from a standard code list (e.g. NAICS 2007 or NACE 2). This schema is a sub-schema of Classification.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "code_scheme_id": {
      "type": "string",
      "description": "An identifier representing industry code scheme. We support the codes listed below",
      "enum": [
        "anz_sic_2006",
        "be_nace_2003",
        "be_nace_2008",
        "be_nace_2025",
        "bg_cea_2008",
        "bo_caeb_2011",
        "ca_qc_cae",
        "dk_db_2007",
        "eu_nace_11",
        "eu_nace_2",
        "fi_tol_2002",
        "fi_tol_2008",
        "fr_naf_2008",
        "gl_gb_2000",
        "in_nic_2004_mca",
        "isic_4",
        "lu_nace_2",
        "me_kd_2010",
        "no_sic_2007",
        "nz_bic_2006",
        "ru_okved_2014",
        "sg_ssic_2015",
        "th_tsic_2009",
        "uk_sic_1992",
        "uk_sic_2003",
        "uk_sic_2007",
        "us_naics_2007",
        "us_naics_2012",
        "us_naics_2017",
        "us_naics_2022",
        "vn_vsic_2007"
      ]
    },
    "start_date": {
      "type": "string",
      "format": "date"
    },
    "end_date": {
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "code",
    "code_scheme_id"
  ]
}
